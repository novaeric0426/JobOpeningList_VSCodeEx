{
  "name": "gamejoblist",
  "displayName": "gamejoblist",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "job-crawler",
          "title": "채용공고 크롤러",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "job-crawler": [
        {
          "id": "jobCrawlerView",
          "icon": "$(refresh)",
          "name": "채용공고 목록"
        }
      ]
    },
    "commands": [
      {
        "command": "jobCrawler.refreshJobs",
        "title": "채용공고 새로고침",
        "icon": "$(refresh)"
      },
      {
        "command": "jobCrawler.openJobURL",
        "title": "채용공고 링크 열기"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "jobCrawler.refreshJobs",
          "when": "view == jobCrawlerView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "cheerio": "^1.0.0"
  }
}
